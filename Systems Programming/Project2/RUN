#! /bin/bash

./BUILD

set -x

NBARBERS=3

NCHAIRS=6

SERVICE_TIME_MS=1200

SERVICE_DEVIATION_MS=200

CUSTOMER_ARRIVALS_MS=400

DURATION_SECONDS=300

./barbers \
    $NBARBERS \
    $NCHAIRS \
    $SERVICE_TIME_MS\
    $SERVICE_DEVIATION_MS \
    $CUSTOMER_ARRIVALS_MS \
    $DURATION_SECONDS \
2> err